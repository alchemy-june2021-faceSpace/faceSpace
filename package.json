{
  "name": "faceSpace",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest --verbose --runInBand --testLocationInResults --setupFiles dotenv/config",
    "test:watch": "npm run test -- --watch",
    "start": "node -r dotenv/config server.js",
    "start:watch": "nodemon -r dotenv/config server.js",
    "setup-db": "node -r dotenv/config setup-db.js",
    "setup-seed-data": "node -r dotenv/config ./data/setup-seed-db.js",
    "setup-heroku": "heroku run npm run setup-db",
    "setup-staging": "staging run npm run setup-seed-data",
    "heroku-config": "heroku addons:create heroku-postgresql:hobby-dev --remote staging && heroku config:set PGSSLMODE=require --remote staging && heroku config:get DATABASE_URL --remote staging"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@types/jest": "^27.0.2",
    "@types/pg": "^8.6.1",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
